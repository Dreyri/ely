#ifndef X
#error X(upper_name, lower_name, pretty_name) must be defined
#endif

X(WHITESPACE, whitespace, "whitespace")
X(TAB, tab, "tab")
X(NEWLINE_CR, newline_cr, "newline")
X(NEWLINE_LF, newline_lf, "newline")
X(NEWLINE_CRLF, newline_crlf, "newline")
X(COMMENT, comment, "comment")

X(EOF, eof, "eof")

X(LPAREN, lparen, "(")
X(RPAREN, rparen, ")")
X(LBRACKET, lbracket, "[")
X(RBRACKET, rbracket, "]")
X(LBRACE, lbrace, "{")
X(RBRACE, rbrace, "}")

X(ID, id, "identifier")

X(INT_LIT, int_lit, "int")
X(FLOAT_LIT, float_lit, "float")
X(CHAR_LIT, char_lit, "char")
X(STRING_LIT, string_lit, "string")
X(KEYWORD_LIT, keyword_lit, "keyword")
X(TRUE_LIT, true_lit, "#t")
X(FALSE_LIT, false_lit, "#f")

#undef X